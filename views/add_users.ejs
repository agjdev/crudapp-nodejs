

    <%- include('partials/header') %>.


  
    <div class="container">
        
    <div class="row ">
        <div class="col-lg-6">
      <h3>Add Users</h3>
      <form class="form-horizontal" action="/add" method="post" enctype="multipart/form-data">
        <div class="form-group">

            <div class="col-sm-10">
              <input type="text" class="form-control" id="name" name="name" placeholder="Enter Full Name">
            </div>
          </div>
        <div class="form-group">
   
          <div class="col-sm-10">
            <input type="email" class="form-control" id="email" name="email" placeholder="Enter Email">
          </div>
        </div>
        <div class="form-group">
          <div class="col-sm-10">
            <input type="text" class="form-control" id="mobile" name="phone" placeholder="Enter Mobile">
          </div>
        </div>
        <div class="form-group">
            <div class="col-sm-10">
              <input type="file" class="form-control" id="file"  name="image" >
            </div>
          </div>
       
        <div class="form-group">
          <div class="col-sm-10">
            <button type="submit"  name="submit" class="btn btn-danger btn-block">Add User</button>
          </div>
        </div>
      </form>





    </div>

    <div class="col-lg-6"> <h3>Add Here</h3></div>

    </div>

    </div>
    
    <%- include('partials/footer') %>.